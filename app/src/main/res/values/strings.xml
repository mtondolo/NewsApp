<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">Merchant Post</string>
    <string name="error_message">An error has occurred. Please try again.</string>
    <string name="action_settings">Settings</string>
    <string name="content_authority">com.example.android.merchantpost</string>
    <string name="settings_title">Settings</string>

    <!-- Strings related to Notification Enabled preference -->
    <string name="pref_enable_notifications_key" translatable="false">enable_notifications</string>
    <string name="pref_enable_notifications_label">News Notifications</string>

    <string name="pref_enable_notifications_true">Enabled</string>
    <string name="pref_enable_notifications_false">Not Enabled</string>

    <string name="pref_last_notification">last_notification</string>



    <!-- The format of the notification we show when the news is updated -->
    <string name="format_notification">
        <xliff:g id="title">%1$s</xliff:g>

    </string>

    <!-- For labelling today's forecast -->
    <string name="today">Today</string>

    <!-- For labelling tomorrow's forecast -->
    <string name="tomorrow">Tomorrow</string>
</resources>
